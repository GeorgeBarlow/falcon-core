{
  "env": {
    "development": {
      "presets": [
        ["env", {
          "targets": { "node": 10 },
          "useBuiltIns": true
        }]
      ]
    },
    "test": {
      "presets": [
        ["env", {
          "targets": { "node": 10 },
          "useBuiltIns": true
        }]
      ]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": ["../../"],
      "alias": {
        "@falcon-client/sql-formatter": "./packages/sql-formatter",
        "@falcon-client/graphql-sequelize": "./packages/graphql-sequelize",
        "@falcon-client/tuql": "./packages/tuql"
      }
    }]
  ]
}
